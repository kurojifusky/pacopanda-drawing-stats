{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".svelte-kit/**", ".vercel/**", ".output/**"]
    },
    "deploy": {
      "dependsOn": ["lint", "build"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
